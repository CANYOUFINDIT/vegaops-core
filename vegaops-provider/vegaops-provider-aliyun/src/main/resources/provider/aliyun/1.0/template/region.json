{
  "vendor": "aliyun",
  "nodeType": "region",
  "componentId": "#componentId",
  "actions": [
    "list"
  ],
  "profile":"T(com.aliyuncs.profile.DefaultProfile).getProfile('cn-hangzhou',#accessKey,#secret)",
  "list": {
    "client": "new com.aliyuncs.DefaultAcsClient(#profile)",
    "method": "getAcsResponse",
    "request": {
      "class": "com.aliyuncs.ecs.model.v20140526.DescribeRegionsRequest"
    },
    "argValues":{
      "request":"#request"
    },
    "output": {
      "success": "#throwable eq null",
      "log": "false",
      "[regionList]:[#result.Regions]": {
        "regionId": "#RegionId",
        "name": "#LocalName"
      }
    }
  }}

